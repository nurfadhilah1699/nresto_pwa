{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off",
        "no-unused-vars": "off",
        "linebreak-style": "off",
        "no-underscore-dangle": "off",
        "no-restricted-globals": "off",
        "no-useless-return": "off",
        "func-names": "off",
        "array-callback-return": "off",
        "no-undef": "off",
        "consistent-return": "off",
        "no-prototype-builtins": "off"
    }
}
